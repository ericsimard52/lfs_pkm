CMD=( make INSTALL_HDR_PATH=$FAKEROOT/$SDN headers_install )
processCmd
[ $? -gt 0 ] && log "PKG|ERROR|Error installing in fakeroot." t && return 1

CMD=( find $FAKEROOT/$SDN/include '(' -name .install -o -name ..install.cmd ')' -delete )
processCmd
CMD=( install -vdm 644 $FAKEROOT/$SDN/usr )
processCmd
CMD=( mv -v $FAKEROOT/$SDN/include $FAKEROOT/$SDN/usr/ )
processCmd
[ $? -gt 0 ] && log "PKG|ERROR|Error installing in fakeroot. Moving to usr." t && return 1
return 0



