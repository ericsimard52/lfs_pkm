log "GEN|INFO|Checking if $FAKEROOT/$SDN/lib exists." t
if [ ! -d $FAKEROOT/$SDN/lib ]; then
   log "GEN|INFO|Creating." t
   CMD=( install -vdm 644 $FAKEROOT/$SDN/lib )
   processCmd
fi
log "GEN|INFO|Moving $FAKEROOT/$SDN/usr/lib/{readline,history} to $FAKEROOT/$SDN/lib" t
CMD=( mv -v $FAKEROOT/$SDN/usr/lib/lib{readline,history}.so.* $FAKEROOT/$SDN/lib )
processCmd
[ $? -gt 0 ] && return 1

log "GEN|INFO|Fixing permissions." t
chmod -v u+w $FAKEROOT/$SDN/lib/lib{readline,history}.so.*
return 0
