CMD=( make DESTDIR=$FAKEROOT/$SDN install )
processCmd
[ $? -gt 0 ] && return 1
CMD=( mkdir $FAKEROOT/$SDN/{bin,lib} )
processCmd
CMD=( mv -v   $FAKEROOT/$SDN/usr/bin/{lzma,unlzma,lzcat,xz,unxz,xzcat} $FAKEROOT/$SDN/bin )
processCmd
CMD=( mv -v $FAKEROOT/$SDN/usr/lib/liblzma.so.* $FAKEROOT/$SDN/lib )
processCmd
return 0


