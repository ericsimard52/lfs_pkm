CMD=( make DESTDIR=$FAKEROOT/$SDN install )
processCmd
[ $? -gt 0 ] && return 1

CMD=( install -vdm755 $FAKEROOT/$SDN/bin )
processCmd
CMD=( mv -v $FAKEROOT/$sdn/usr/bin/"{cat,chgrp,chmod,chown,cp,date,dd,df,echo}" $FAKEROOT/$sdn/bin )
processCmd
CMD=( mv -v $FAKEROOT/$sdn/usr/bin/"{false,ln,ls,mkdir,mknod,mv,pwd,rm}" $FAKEROOT/$sdn/bin )
processCmd
CMD=( mv -v $FAKEROOT/$sdn/usr/bin/"{rmdir,stty,sync,true,uname}" $FAKEROOT/$sdn/bin )
processCmd
CMD=( mv -v $FAKEROOT/$sdn/usr/bin/chroot $FAKEROOT/$sdn/usr/sbin )
processCmd
CMD=( install -vdm755 $FAKEROOT/$sdn/usr/share/man/man8 )
processCmd
CMD=( mv -v $FAKEROOT/$sdn/usr/share/man/man1/chroot.1 $FAKEROOT/$sdn/usr/share/man/man8/chroot.8 )
processCmd
sed -i s/\"1\"/\"8\"/1 $FAKEROOT/$sdn/usr/share/man/man8/chroot.8
CMD=( mv -v $FAKEROOT/$sdn/usr/bin/"{head,sleep,nice}" $FAKEROOT/$sdn/bin )
processCmd
  
return 0
